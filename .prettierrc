{
	"doubleQuote": true,
	"trailingComma": "none",
	"semi": false,
	"tabWidth": 2,
	"useTabs": true,
	"bracketSpacing": true,
	"arrowParens": "avoid",
	"bracketSameLine": true,
	"quoteProps": "as-needed",
	"printWidth": 120,
	"endOfLine": "auto",
	"singleAttributePerLine": false,
	"proseWrap": "always"
}
