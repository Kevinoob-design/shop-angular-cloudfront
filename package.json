{
	"name": "app",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"prepare": "husky install",
		"lint:format": "npx --no-install lint-staged && npx --no-install pretty-quick --staged",
		"spell": "cspell \"**\" --gitignore --no-summary --no-progress --no-must-find-files",
		"spell:in": "cspell stdin",
		"client:deploy": "yarn build && npx --no-install serverless client deploy --no-confirm"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^17.2.4",
		"@angular/cdk": "^17.2.2",
		"@angular/common": "^17.2.4",
		"@angular/compiler": "^17.2.4",
		"@angular/core": "^17.2.4",
		"@angular/forms": "^17.2.4",
		"@angular/material": "^17.2.2",
		"@angular/platform-browser": "^17.2.4",
		"@angular/platform-browser-dynamic": "^17.2.4",
		"@angular/router": "^17.2.4",
		"bootstrap": "^5.3.3",
		"rxjs": "^7.8.0",
		"tslib": "^2.5.0",
		"zone.js": "^0.14.4"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^17.2.3",
		"@angular-eslint/builder": "^17.2.1",
		"@angular-eslint/eslint-plugin": "^17.2.1",
		"@angular-eslint/eslint-plugin-template": "^17.2.1",
		"@angular-eslint/schematics": "17.2.1",
		"@angular-eslint/template-parser": "^17.2.1",
		"@angular/cli": "^17.2.3",
		"@angular/compiler-cli": "^17.2.4",
		"@commitlint/cli": "^19.0.3",
		"@commitlint/config-conventional": "^19.0.3",
		"@cspell/dict-es-es": "^2.2.0",
		"@types/jasmine": "^5.1.4",
		"@types/node": "^20.11.25",
		"@typescript-eslint/eslint-plugin": "^7.1.1",
		"@typescript-eslint/parser": "^7.1.1",
		"cspell": "^8.6.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jasmine": "^4.1.3",
		"eslint-plugin-jsdoc": "^48.2.1",
		"eslint-plugin-prefer-arrow": "latest",
		"eslint-plugin-rxjs": "^5.0.2",
		"eslint-plugin-simple-import-sort": "^12.0.0",
		"eslint-plugin-sonarjs": "^0.24.0",
		"husky": "^9.0.11",
		"jasmine-core": "~5.1.2",
		"karma": "~6.4.3",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.1",
		"karma-jasmine": "~5.1.0",
		"karma-spec-reporter": "0.0.36",
		"lint-staged": "^15.2.2",
		"prettier": "^3.2.5",
		"pretty-quick": "^4.0.0",
		"protractor": "~7.0.0",
		"serverless": "^3.38.0",
		"serverless-finch": "^4.0.4",
		"serverless-single-page-app-plugin": "^1.0.4",
		"ts-node": "^10.9.1",
		"typescript": "~5.3.0"
